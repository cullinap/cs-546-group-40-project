<article>
  <h2 class="header-titles">Recent Matches and Clips</h2>
  <div class="container">
    {{#if espn}}
      <h2>ESPN</h2>
      <ul>
        {{#each espn}}
          <li><a
              href="https://youtu.be/{{id.videoId}}"
              target="_blank"
              class="text link"
            >{{snippet.title}}</a></li>
        {{/each}}
      </ul>
    {{/if}}
    {{#if nbc}}
      <h2>NBC Sports</h2>
      <ul>
        {{#each nbc}}
          <li><a
              href="https://youtu.be/{{id.videoId}}"
              target="_blank"
              class="text link"
            >{{snippet.title}}</a></li>
        {{/each}}
      </ul>
    {{/if}}
  </div>
</article>