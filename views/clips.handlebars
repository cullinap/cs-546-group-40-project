<article>
  <p>Recent Matches and Clips</p>
  {{#if espn}}
    <h2>ESPN</h2>
    <ul>
      {{#each espn}}
        <li><a
            href="https://youtu.be/{{id.videoId}}"
            target="_blank"
          >{{snippet.title}}</a></li>
      {{/each}}
    </ul>
  {{/if}}
  {{#if nbc}}
    <h2>NBC Sports</h2>
    <ul>
      {{#each nbc}}
        <li><a
            href="https://youtu.be/{{id.videoId}}"
            target="_blank"
          >{{snippet.title}}</a></li>
      {{/each}}
    </ul>
  {{/if}}
</article>
